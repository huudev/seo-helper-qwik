{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "clean": "rmdir /s /q dist 2> nul || echo Cleaned",
    "dev.qwik": "vite --mode ssr",
    "dev.qwik.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "watch.sw": "vite build -c vite.service-worker.ts -w -m development",
    "watch.delegate": "node delegate.js",
    "dev.ext": "run-p clean dev.qwik watch.sw watch.delegate",
    "build.bs.css": "sass --no-source-map ./public/bootstrap.scss:./src/bootstrap.css",
    "build.qwik": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.sw": "vite build -c vite.service-worker.ts",
    "build.transfrom": "node transfrom.js",
    "build.ext": "run-s clean build.qwik build.sw build.transfrom",
    "upload": "node upload.js",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint --ignore-pattern service-worker.d.ts \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.5",
    "@builder.io/qwik-city": "^1.1.5",
    "@types/chrome": "^0.0.237",
    "@types/eslint": "8.40.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.41.0",
    "eslint-plugin-qwik": "^1.1.5",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "undici": "5.22.1",
    "vite": "4.3.9",
    "vite-plugin-conditional-compiler": "^0.1.1",
    "vite-tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@modular-forms/qwik": "^0.19.1",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "jszip": "^3.10.1",
    "node-html-parser": "^6.1.5",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-tsconfig-paths": "^1.5.1",
    "sass": "^1.64.1",
    "termit": "^0.4.4"
  }
}
