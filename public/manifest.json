{
  "manifest_version": 3,
  "name": "Seo helper",
  "version": "3.0.1",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "commands",
    "clipboardRead",
    "clipboardWrite",
    "scripting",
    "offscreen",
    "search"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "commands": {
    "search": {
      "suggested_key": {
        "default": "Alt+S"
      },
      "description": "Tìm kiếm google theo clipboard"
    }
  }
}